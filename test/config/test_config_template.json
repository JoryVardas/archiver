{
  "general": {
    "file_read_sizes": [
      104857600
    ]
  },
  "stager": {
    "stage_directory": "${ARCHIVER_TEST_CONFIG_STAGE_DIRECTORY_VALUE}"
  },
  "archive": {
    "archive_directory": "${ARCHIVER_TEST_CONFIG_ARCHIVE_DIRECTORY_VALUE}",
    "temp_archive_directory": "${ARCHIVER_TEST_CONFIG_ARCHIVE_TEMP_DIRECTORY_VALUE}",
    "target_size": 10240,
    "single_archive_size": 5120
  },
  "database": {
    "user": "${ARCHIVER_TEST_CONFIG_DATABASE_USERNAME_VALUE}",
    "password": "${ARCHIVER_TEST_CONFIG_DATABASE_PASSWORD_VALUE}",
    "location": {
      "host": "${ARCHIVER_TEST_CONFIG_DATABASE_HOST_VALUE}",
      "port": ${ARCHIVER_TEST_CONFIG_DATABASE_PORT_VALUE},
      "schema": "archiver"
    },
    "options": [
      "parseTime=true"
    ]
  },
  "aws": {
    "access_key": "access_key",
    "secret_key": "secret_key"
  }
}
